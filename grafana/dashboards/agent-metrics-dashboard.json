{
  "dashboard": {
    "uid": "agent-metrics",
    "title": "Agent Metrics Dashboard",
    "tags": [
      "agents",
      "tasks",
      "monitoring"
    ],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Tasks Completed",
        "type": "stat",
        "datasource": {
          "type": "influxdb",
          "uid": "InfluxDB"
        },
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "query": "from(bucket: \"agent_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"agent_metrics\")\n  |> filter(fn: (r) => r[\"_field\"] == \"tasks_completed\")\n  |> sum()",
            "refId": "A"
          }
        ],
        "transparent": false,
        "options": {},
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Agent Status",
        "type": "table",
        "datasource": {
          "type": "influxdb",
          "uid": "InfluxDB"
        },
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "query": "from(bucket: \"agent_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"agent_metrics\")\n  |> filter(fn: (r) => r[\"_field\"] == \"status\")\n  |> last()\n  |> group(columns: [\"agent\"])",
            "refId": "A"
          }
        ],
        "transparent": false,
        "options": {}
      },
      {
        "id": 3,
        "title": "Task Completion Rate",
        "type": "graph",
        "datasource": {
          "type": "influxdb",
          "uid": "InfluxDB"
        },
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "query": "from(bucket: \"agent_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"agent_metrics\")\n  |> filter(fn: (r) => r[\"_field\"] == \"tasks_completed\")\n  |> aggregateWindow(every: 1m, fn: sum, createEmpty: false)\n  |> derivative(unit: 1m, nonNegative: true)",
            "refId": "A"
          }
        ],
        "transparent": false,
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        }
      },
      {
        "id": 4,
        "title": "Agent Resource Usage",
        "type": "heatmap",
        "datasource": {
          "type": "influxdb",
          "uid": "InfluxDB"
        },
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "query": "from(bucket: \"agent_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"agent_metrics\")\n  |> filter(fn: (r) => r[\"_field\"] == \"resource_usage\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)",
            "refId": "A"
          }
        ],
        "transparent": false,
        "options": {},
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        }
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "datasource": {
          "type": "influxdb",
          "uid": "InfluxDB"
        },
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "query": "from(bucket: \"agent_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"agent_metrics\")\n  |> filter(fn: (r) => r[\"_field\"] == \"errors\")\n  |> aggregateWindow(every: v.windowPeriod, fn: sum, createEmpty: false)",
            "refId": "A"
          }
        ],
        "transparent": false,
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        }
      }
    ],
    "schemaVersion": 37,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "links": [],
    "liveNow": false,
    "weekStart": ""
  },
  "overwrite": true
}
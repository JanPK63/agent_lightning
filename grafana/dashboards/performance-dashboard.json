{
  "dashboard": {
    "uid": "agent-performance",
    "title": "System Performance Monitoring",
    "tags": [
      "performance",
      "monitoring",
      "system"
    ],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage",
        "type": "graph",
        "datasource": {
          "type": "influxdb",
          "uid": "InfluxDB"
        },
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "query": "from(bucket: \"performance_metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"performance_metrics\")\n  |> filter(fn: (r) => r[\"metric_type\"] == \"cpu\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)",
            "refId": "A"
          }
        ],
        "transparent": false,
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 60
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        }
      },

{
  "dashboard": {
    "id": null,
    "title": "Encryption Audit Dashboard",
    "tags": ["encryption", "security", "audit"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Decrypt Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(encryption_decrypt_errors_total[5m]) / rate(encryption_decrypt_operations_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.05
                },
                {
                  "color": "red",
                  "value": 0.1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Audit Log Coverage",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(encryption_audit_logs_total[5m]) / rate(encryption_operations_total[5m]) * 100",
            "legendFormat": "Coverage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99.9
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Encryption Operations by Type",
        "type": "barchart",
        "targets": [
          {
            "expr": "rate(encryption_operations_total[5m])",
            "legendFormat": "{{operation}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Key Usage by Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "rate(encryption_operations_total[5m])",
            "legendFormat": "{{key_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Encryption Performance Impact",
        "type": "graph",
        "targets": [
          {
            "expr": "(rate(encryption_query_duration_seconds_sum[5m]) / rate(encryption_query_duration_seconds_count[5m])) / (rate(query_duration_seconds_sum{encrypted=\"false\"}[5m]) / rate(query_duration_seconds_count{encrypted=\"false\"}[5m]))",
            "legendFormat": "Performance Ratio"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Key Expiration Status",
        "type": "table",
        "targets": [
          {
            "expr": "(encryption_key_expires_at - time()) / 86400",
            "legendFormat": "Days until expiry"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 30
                },
                {
                  "color": "red",
                  "value": 7
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Recent Audit Events",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"encryption-service\"} | json | operation != \"\"",
            "legendFormat": "{{operation}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
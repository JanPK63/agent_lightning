{
  "dashboard": {
    "id": null,
    "title": "RL Agent Analytics",
    "tags": ["rl", "agents"],
    "panels": [
      {
        "id": 1,
        "title": "Active RL Agents",
        "type": "stat",
        "targets": [{"expr": "rl_sessions_active"}],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 3},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Agent Task Success",
        "type": "graph",
        "targets": [
          {"expr": "agent_tasks_total", "legendFormat": "{{agent}} - Total"},
          {"expr": "agent_errors_total", "legendFormat": "{{agent}} - Errors"}
        ],
        "gridPos": {"h": 8, "w": 18, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Agent Response Times",
        "type": "graph", 
        "targets": [
          {"expr": "histogram_quantile(0.95, agent_task_duration_seconds)", "legendFormat": "95th percentile"},
          {"expr": "histogram_quantile(0.50, agent_task_duration_seconds)", "legendFormat": "50th percentile"}
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      }
    ]
  }
}